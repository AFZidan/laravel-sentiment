{
  "name": "ahmedzidan/laravel-sentiment",
  "description": "Sentiment analyzer Package for Laravel Framework",
  "keywords": [
    "laravel",
    "framework",
    "sentiment analysis",
    "sentiment",
    "analysis",
    "zidan",
    "Ahmed Zidan",
    "ahmedzidan"
  ],
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Ahmed Zidan",
      "email": "php.ahmedzidan@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6.0",
    "illuminate/support": "~5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Zidan\\LaravelSentiment\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zidan\\LaravelSentiment\\SentimentServiceProvider"
      ],
      "aliases": {
        "SentimentAnalysis": "Zidan\\LaravelSentiment\\Facades\\Analise"
      }
    }
  }
}
